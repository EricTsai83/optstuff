{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "optstuff-marketing": {
      "development": {
        "fallback": "optstuff.vercel.app",
        "local": {
          "port": 3000
        }
      }
    },
    "optstuff-dashboard": {
      "development": {
        "local": {
          "port": 3001
        }
      },
      "routing": [
        {
          "paths": [
            "/dashboard/:path*",
            "/dashboard-static/:path*",
            "/sign-in/:path*"
          ]
        }
      ]
    }
  },
  "options": {
    "localProxyPort": 3024
  }
}
